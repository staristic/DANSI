{
 "name": "DANSI",
 "version": "0.2.0",
 "description": "Draw ANSI!",
 "main": "main.js",
 "scripts": {
  "addon": "cd addon && node-gyp rebuild --target=16.0.8 --arch=x64 --dist-url=https://electronjs.org/headers",
  "build": "webpack",
  "dev": "webpack && electron . dev",
  "lint": "eslint --fix src/**/*.js controllers/* webpack.config.js main.js",
  "pack": "electron-builder",
  "pack-mac": "electron-builder -mw",
  "postinstall": "electron-builder install-app-deps",
  "start": "webpack && electron .",
  "stylelint": "stylelint src/**/*.scss"
 },
 "devDependencies": {
  "css-loader": "^6.6.0",
  "electron": "^16.0.8",
  "electron-builder": "^22.14.13",
  "electron-rebuild": "^3.2.7",
  "eslint": "^8.8.0",
  "eslint-config-google": "^0.14.0",
  "mini-css-extract-plugin": "^2.5.3",
  "sass": "^1.49.7",
  "sass-loader": "^12.4.0",
  "style-loader": "^3.3.1",
  "stylelint": "^14.3.0",
  "stylelint-config-standard": "^24.0.0",
  "stylelint-scss": "^4.1.0",
  "webpack": "^5.76.0",
  "webpack-cli": "^4.9.2"
 },
 "dependencies": {
  "html-loader": "^3.1.0",
  "html-webpack-plugin": "^5.5.0"
 },
 "build": {
  "appId": "0.1",
  "directories": {
   "output": "pack"
  },
  "icon": "assets/icons/DANSI.png",
  "win": {
   "target": [
    {
     "target": "nsis",
     "arch": [
      "x64",
      "ia32"
     ]
    }
   ]
  },
  "mac": {
   "target": [
    {
     "target": "dmg",
     "arch": [
      "x64"
     ]
    }
   ]
  }
 }
}
